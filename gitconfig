[color]
    branch      = auto
    diff        = auto
    interactive = auto
    status      = auto
    ui          = auto

[color "status"]
    added     = green
    changed   = red bold
    untracked = magenta bold

[color "branch"]
    remote = yellow

[alias]
    a   = add .
    ai  = add -i .
    b   = branch
    bm  = branch -m
    brm = branch -D
    cb  = checkout -b
    c  = commit
    cl  = clean
    cm  = commit -m
    co  = checkout
    d   = diff
    dh  = diff --compaction-heuristic
    dhc = diff --compaction-heuristic --word-diff=color --word-diff-regex=.
    dhw = diff --compaction-heuristic --word-diff=color
    dw  = diff --word-diff=color
    g   = grep
    ls  = log @^ --name-only --oneline
    pl  = pull
    ps  = push
    r   = reset
    s   = show
    st  = status

